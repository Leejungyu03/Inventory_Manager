<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.manager.product.dao.ProductDAO">
 	<insert id="insertProduct" parameterType="map">
 		INSERT INTO `product`
 		(
 			`userId`
 			,`title`
 			,`content`
 			,`stock`
 			,`imageUrl`
 			,`createdAt`
 			,`updatedAt`
 		)
 		VALUES
 		(
 			#{userId}
 			,#{title}
 			,#{content}
 			,#{stock}
 			,#{imageUrl}
 			,NOW()
 			,NOW()
 		)
 	</insert>
 </mapper>